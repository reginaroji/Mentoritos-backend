package org.mentoritos.elearning.users;

import java.time.LocalDateTime;
import java.util.Date;

public class StudentsProfile extends UserInfo {
	private int idUserProfileStudy;
	private String description;
	private String education;
	private String interests;

	public StudentsProfile(int idUser, String username, String email, String password, String uid, int id, String name,
			String lastname, String profileImage, String location, Date birthday, String phone,
			LocalDateTime registration, int idUserProfileStudy, String description, String education,
			String interests) {
		super(idUser, username, email, password, uid, id, name, lastname, profileImage, location, birthday, phone,
				registration);
		this.idUserProfileStudy = idUserProfileStudy;
		this.description = description;
		this.education = education;
		this.interests = interests;
	}


	public int getIdUserProfileStudy() {
		return idUserProfileStudy;
	}


	public void setIdUserProfileStudy(int idUserProfileStudy) {
		this.idUserProfileStudy = idUserProfileStudy;
	}


	public String getDescription() {
		return description;
	}


	public void setDescription(String description) {
		this.description = description;
	}


	public String getEducation() {
		return education;
	}


	public void setEducation(String education) {
		this.education = education;
	}


	public String getInterests() {
		return interests;
	}


	public void setInterests(String interests) {
		this.interests = interests;
	}


	@Override
	public String toString() {
		return "UsersProfile [idUserProfileStudy=" + idUserProfileStudy + ", description=" + description
				+ ", education=" + education + ", interests=" + interests + "]";
	}
	
	
}
